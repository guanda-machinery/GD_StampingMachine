                                                </MultiDataTrigger>

                                            </Style.Triggers>
                                        </Style>
                                    </local:FunctionToggleUserControl.Style>
                                </local:FunctionToggleUserControl>

                                <local:FunctionToggleUserControl ContentPadding="5"
