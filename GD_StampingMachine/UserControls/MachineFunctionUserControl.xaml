                    <StackPanel Orientation="Horizontal">
                        <StackPanel Margin="5,0,20,0">
                                <local:FunctionToggleUserControl  ContentPadding="5"
                                    IsChecked="{Binding Feeding_Component_Button_IsChecked}" 
                                                                             ControlTitleText="{DynamicResource Text_Feeding_Component}" >
                                    <local:FunctionToggleUserControl.Style>
                                        <Style TargetType="local:FunctionToggleUserControl" BasedOn="{StaticResource FunctionToggleUserControlKey}" >
                                            <Setter Property="ImageSource" Value="{StaticResource Feeding_Component_BlackDrawingImage}" />
                                            <Style.Triggers>
                                                <Trigger Property="IsChecked" Value="True">
                                                    <Setter Property="ImageSource" Value="{StaticResource Feeding_Component_BlueDrawingImage}"/>
                                                </Trigger>
                                                <MultiDataTrigger>
                                                    <MultiDataTrigger.Conditions>
                                                        <Condition Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="false" />
                                                        <Condition Binding="{Binding RelativeSource={RelativeSource Mode=Self} ,Path=IsChecked}" Value="False" />
                                                    </MultiDataTrigger.Conditions>
                                                    <Setter Property="ImageSource" Value="{StaticResource Feeding_Component_WhiteDrawingImage}"/>

                                                </MultiDataTrigger>

                                            </Style.Triggers>
                                        </Style>
                                    </local:FunctionToggleUserControl.Style>
                                </local:FunctionToggleUserControl>



                                <local:FunctionToggleUserControl ContentPadding="5"
                                    IsChecked="{Binding QRCode_Component_Button_IsChecked}"
                                   
                                ControlTitleText="{DynamicResource Text_QRCode_Component}" >
                                    <local:FunctionToggleUserControl.Style>
                                        <Style TargetType="local:FunctionToggleUserControl" BasedOn="{StaticResource FunctionToggleUserControlKey}" >
                                            <Setter Property="ImageSource" Value="{StaticResource QRCode_Component_BlackDrawingImage}" />
                                            <Style.Triggers>
                                                <Trigger Property="IsChecked" Value="True">
                                                    <Setter Property="ImageSource" Value="{StaticResource QRCode_Component_BlueDrawingImage}"/>
                                                </Trigger>
                                                <MultiDataTrigger>
                                                    <MultiDataTrigger.Conditions>
                                                        <Condition Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="false" />
                                                        <Condition Binding="{Binding RelativeSource={RelativeSource Mode=Self} ,Path=IsChecked}" Value="False" />
                                                    </MultiDataTrigger.Conditions>
                                                    <Setter Property="ImageSource" Value="{StaticResource QRCode_Component_WhiteDrawingImage}"/>
                                                </MultiDataTrigger>

                                            </Style.Triggers>
                                        </Style>
                                    </local:FunctionToggleUserControl.Style>
                                </local:FunctionToggleUserControl>

                                <local:FunctionToggleUserControl ContentPadding="5" IsChecked="{Binding Stamping_Component_Button_IsChecked}"
                                ControlTitleText="{DynamicResource Text_Stamping_Component}" >
                                    <local:FunctionToggleUserControl.Style>
