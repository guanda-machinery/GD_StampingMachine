<UserControl x:Class="GD_StampingMachine.UserControls.MachineMonitorUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GD_StampingMachine"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="800" 
                      
             
                    xmlns:CalcB="clr-namespace:CalcBinding;assembly=CalcBinding" 
               xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" 
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"                       
             xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling"          
             xmlns:GD_ViewModels="clr-namespace:GD_StampingMachine.ViewModels"
             xmlns:GD_ProductSettingViewModels="clr-namespace:GD_StampingMachine.ViewModels.ProductSetting"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:GD_StampingMachine_UserControls="clr-namespace:GD_StampingMachine.UserControls"
             
             xmlns:GD_Singletons="clr-namespace:GD_StampingMachine.Singletons" 
             
             xmlns:GD_SchematicDiagram="clr-namespace:GD_StampingMachine.UserControls.NumberSettingSchematicDiagram" 
             xmlns:GD_ParameterSettingViewModels="clr-namespace:GD_StampingMachine.ViewModels.ParameterSetting" 
             xmlns:GD_Common="clr-namespace:GD_CommonLibrary;assembly=GD_CommonLibrary" xmlns:c="clr-namespace:CalcBinding;assembly=CalcBinding"
             d:DataContext="{d:DesignInstance Type=GD_ViewModels:MachineMonitorViewModel}"
             
              xmlns:sys="clr-namespace:System;assembly=mscorlib" 
             xmlns:GD_Popup="clr-namespace:GD_CommonLibrary.GD_Popup;assembly=GD_CommonLibrary"
             Name="MachineMonitorUC"
             >
    

        <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="auto" MinHeight="50" MaxHeight="{Binding ElementName=PrimaryColorZone, Path=ActualHeight}"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <materialDesign:ColorZone  Name="PrimaryColorZone"
                Mode="PrimaryDark"  >
            <materialDesign:ColorZone.Effect>
                <DropShadowEffect  ShadowDepth="2"
      Direction="270"
      Color="Black"
      Opacity="0.5"
     BlurRadius="6"/>
            </materialDesign:ColorZone.Effect>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                </Grid.RowDefinitions>
                <!--TextBlock  Text="{Binding ProjectDistributeVMSelected.ProjectDistributeName}" Grid.ColumnSpan="2" FontSize="30"/-->
                <StackPanel Grid.Row="0"  Grid.Column="1"  Orientation="Horizontal" HorizontalAlignment="Center" >
                        <Border  
                            BorderBrush="#e82127" BorderThickness="0,0,0,5" CornerRadius="0" Width="200" Height="200"  >
                            <!--Image Source="pack://application:,,,/image/Machine/Machine_QR_Dark.svg"  Margin="5"/-->
                            <Image Margin="5">
                                <Image.Style>
                                    <Style TargetType="Image">
                                        <Setter Property="Source" Value="{StaticResource QR_Machine_DarkDrawingImage}"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                <Setter Property="Source" Value="{StaticResource  QR_Machine_LightDrawingImage}"/>
                                            </DataTrigger>

                                        </Style.Triggers>
                                    </Style>

                                </Image.Style>
                            </Image>
                        </Border>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <StackPanel.Resources>
                                <Style TargetType="materialDesign:PackIcon">
                                    <Setter Property="Width" Value="50"/>
                                    <Setter Property="Height" Value="50"/>
                                    <Setter Property="Foreground" Value="#e82127"/>
                                <Setter Property="Margin" Value="5"/>
                            </Style>
                        </StackPanel.Resources>
                        <materialDesign:PackIcon Kind="ChevronDoubleRight" />
                            <materialDesign:PackIcon Kind="ChevronDoubleRight" />
                        </StackPanel>
                    <Border  BorderBrush="#ff9900" BorderThickness="0,0,0,5" CornerRadius="0" Width="200" Height="200"  >
                            <Image Margin="5">
                                <Image.Style>
                                    <Style TargetType="Image">
                                        <Setter Property="Source" Value="{StaticResource Stamper_DarkDrawingImage}"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                <Setter Property="Source" Value="{StaticResource  Stamper_LightDrawingImage}"/>
                                            </DataTrigger>

                                        </Style.Triggers>
                                    </Style>

                                </Image.Style>
                            </Image>
                        </Border>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <StackPanel.Resources>
                                <Style TargetType="materialDesign:PackIcon">
                                    <Setter Property="Width" Value="50"/>
                                    <Setter Property="Height" Value="50"/>
                                    <Setter Property="Foreground" Value="#ff9900"/>
                                <Setter Property="Margin" Value="5"/>
                            </Style>
                        </StackPanel.Resources>
                        <materialDesign:PackIcon Kind="ChevronDoubleRight" />
                            <materialDesign:PackIcon Kind="ChevronDoubleRight" />
                        </StackPanel>
                        <Border  BorderBrush="#25cb55" BorderThickness="0,0,0,5" CornerRadius="0" Width="200" Height="200"  >
                            <Image Margin="5">
                                <Image.Style>
                                    <Style TargetType="Image">
                                        <Setter Property="Source" Value="{StaticResource Cutter_DarkDrawingImage}"/>

                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                <Setter Property="Source" Value="{StaticResource  Cutter_LightDrawingImage}"/>
                                            </DataTrigger>

                                        </Style.Triggers>
                                    </Style>

                                </Image.Style>
                            </Image>
                        </Border>
                </StackPanel>

                <Border Grid.Row="1" Margin="10" Background="{DynamicResource PrimaryHueLightForegroundBrush}"/>

                    <Grid Grid.ColumnSpan="3"    Grid.Row="2" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.ColumnSpan="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="95"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition MinHeight="110"/>
                            </Grid.RowDefinitions>
                            <Border Background="White" Grid.ColumnSpan="2" Margin="0,0,0,0" />
                            <Border Width="50" HorizontalAlignment="Right" Margin="0,0,345,0" >
                                <Border.Style>
                                    <Style TargetType="Border" >
                                        <Setter Property="Background" Value="LightGray"/>
                                        <Style.Triggers>
                                            <MultiDataTrigger>
                                                <MultiDataTrigger.Conditions>
                                                    <Condition  Binding="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance}, Path=IsConnected}" 
                                                     Value="true" />
                                                    <Condition  Binding="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance}, Path=Cylinder_HydraulicEngraving_IsStopDown}" 
                                                     Value="true" />
                                                </MultiDataTrigger.Conditions>
                                                <Setter Property="Background"  Value="Green"/>
                                            </MultiDataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Border.Style>
                            </Border>
                            <Border Grid.Column="1" Margin="-0,0,0,0" Width="50"  HorizontalAlignment="Right" >
                                <Border.Style>
                                    <Style TargetType="Border" >
                                        <Setter Property="Background" Value="Gray"/>
                                        <Style.Triggers>
                                            <MultiDataTrigger>
                                                <MultiDataTrigger.Conditions>
                                                    <Condition  Binding="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance}, Path=IsConnected}" 
                                                     Value="true" />
                                                    <Condition  Binding="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance}, Path=Cylinder_HydraulicCutting_IsCutPoint}" 
                                                     Value="true" />
                                                </MultiDataTrigger.Conditions>
                                                <Setter Property="Background"  Value="Green"/>
                                            </MultiDataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Border.Style>

                            </Border>
                        </Grid>
                        <ItemsControl  Grid.Column="0"
                            FlowDirection="RightToLeft"  VirtualizingPanel.IsVirtualizing="True" VirtualizingPanel.VirtualizationMode="Standard"
                                ItemsSource="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance},Path=MachineSettingBaseCollection, IsAsync=True}"
                                                             ScrollViewer.VerticalScrollBarVisibility="Disabled"     
                                 ScrollViewer.HorizontalScrollBarVisibility="Disabled"  >
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <Grid FlowDirection="LeftToRight">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Border  Margin="0,4,5,5"  x:Name="MachiningPlateBorder"
                                                                                        BorderThickness="1" BorderBrush="Black" CornerRadius="5"
                                                                                        HorizontalAlignment="Center"
                                                                                        >
                                            <Border.Style>
                                                <Style TargetType="Border" >
                                                    <Setter Property="Background" Value="{DynamicResource NoneIsFinishSolidColorBrush}"/>
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding DataMatrixIsFinish }" Value="true">
                                                            <Setter Property="Background" Value="{DynamicResource DataMatrixIsFinishSolidColorBrush}"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding EngravingIsFinish}" Value="true">
                                                            <Setter Property="Background" Value="{DynamicResource ShearingIsFinishSolidColorBrush}"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding ShearingIsFinish}" Value="true">
                                                            <Setter Property="Background" Value="{DynamicResource EngravingIsFinishSolidColorBrush}"/>
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Border.Style>
                                            <Grid Opacity="1" HorizontalAlignment="Center" VerticalAlignment="Center"    >
                                                <Grid.LayoutTransform>
                                                    <TransformGroup>
                                                        <RotateTransform Angle="90"/>
                                                    </TransformGroup>
                                                </Grid.LayoutTransform>
                                                <ToggleButton Padding="0" Margin="1" x:Name="MachiningFormTButton" Opacity="0.95">
                                                    <ToggleButton.Style>
                                                        <Style TargetType="ToggleButton" BasedOn="{StaticResource GD_MainToggleButton}">
                                                            <Setter Property="Width" Value="auto"/>
                                                            <Setter Property="Height" Value="auto"/>
                                                            <Setter Property="BorderThickness" Value="0"/>
                                                            <Setter Property="Template">
                                                                <Setter.Value>
                                                                    <ControlTemplate TargetType="ToggleButton">
                                                                        <Grid>
                                                                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                                        </Grid>
                                                                    </ControlTemplate>
                                                                </Setter.Value>
                                                            </Setter>
                                                            <Style.Triggers>
                                                                <Trigger Property="IsMouseOver" Value="True">
                                                                    <Setter Property="IsChecked" Value="True"/>
                                                                </Trigger>
                                                                <Trigger Property="IsMouseOver" Value="False">
                                                                    <Setter Property="IsChecked" Value="False"/>
                                                                </Trigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </ToggleButton.Style>
                                                    <GD_SchematicDiagram:QRPlateRowDiagramControl   
                                                        IsEnabled="False"    PlateRowBackground="Transparent"
                                                                        DataContext="{Binding SettingBaseVM}" RedMeasurementLineVisibility="Collapsed" >
                                                        <GD_SchematicDiagram:QRPlateRowDiagramControl.LayoutTransform>
                                                            <TransformGroup>
                                                                <ScaleTransform ScaleX="1" ScaleY="1"/>
                                                            </TransformGroup>
                                                        </GD_SchematicDiagram:QRPlateRowDiagramControl.LayoutTransform>
                                                    </GD_SchematicDiagram:QRPlateRowDiagramControl>
                                                </ToggleButton>
                                                <GD_Popup:NonTopmostPopup StaysOpen="True" 
                                                                        PopupAnimation="Fade"   FlowDirection="LeftToRight"  
                                                                        Placement="Bottom"    PlacementRectangle="-20,40,0,0"
                                                                        PlacementTarget="{Binding ElementName=MachiningFormTButton}"  
                                                                        HorizontalAlignment="Center" 
                                                                        IsOpen="{Binding ElementName=MachiningFormTButton, Path=IsChecked, Mode=TwoWay}" >

                                                    <Grid>
                                                        <Border   Name="theTransform" BorderBrush="Black" BorderThickness="1" CornerRadius="10" Opacity="0.95"
                                                                RenderTransformOrigin="0.5,0.5" >
                                                            <Border.Style>
                                                                <Style TargetType="Border">
                                                                    <Setter Property="LayoutTransform">
                                                                        <Setter.Value>
                                                                            <RotateTransform Angle="-90"/>
                                                                        </Setter.Value>
                                                                    </Setter>
                                                                </Style>
                                                            </Border.Style>
                                                            <Grid>
                                                                <GD_SchematicDiagram:QRPlateRowDiagramControl     PlateRowBackground="{Binding ElementName=MachiningPlateBorder ,Path=Background}"
                                                                    DataContext="{Binding SettingBaseVM}" RedMeasurementLineVisibility="Collapsed" >
                                                                    <GD_SchematicDiagram:QRPlateRowDiagramControl.LayoutTransform>
                                                                        <TransformGroup>
                                                                            <ScaleTransform ScaleX="4" ScaleY="4"/>
                                                                        </TransformGroup>
                                                                    </GD_SchematicDiagram:QRPlateRowDiagramControl.LayoutTransform>
                                                                </GD_SchematicDiagram:QRPlateRowDiagramControl>
                                                            </Grid>
                                                        </Border>
                                                    </Grid>
                                                </GD_Popup:NonTopmostPopup>




                                                <!--
                                                <Grid DataContext="{Binding SettingBaseVM}">
                                                    
                                                    
                                                    <GD_SchematicDiagram:PlateRowDiagramViewUserControl  
                                                                                                                                 Visibility="{Binding Converter={GD_Common:ObjectIsEqualToVisibilityConverter IsTypeJudge=True} , ConverterParameter={x:Type GD_ParameterSettingViewModels:NumberSettingViewModel}}"
                                                                                     PlateRowBackground="Transparent"
                                                                                                    RedMeasurementLineVisibility="Collapsed" />
                                                    <GD_SchematicDiagram:PlateRowDiagramWithQRViewUserControl 
                                                                                                                                          Visibility="{Binding Converter={GD_Common:ObjectIsEqualToVisibilityConverter IsTypeJudge=True} , ConverterParameter={x:Type GD_ParameterSettingViewModels:QRSettingViewModel}}"
                                                                  PlateRowBackground="Transparent"
                                                                                                    RedMeasurementLineVisibility="Collapsed" />
                                                </Grid>-->
                                                
                                                
                                            </Grid>
                                        </Border>


                                    </Grid>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Horizontal" />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                        </ItemsControl>
                    </Grid>
                <StackPanel Orientation="Horizontal" Grid.Row="3" Grid.ColumnSpan="3">

                        <Button  Margin="5,0" Padding="5,2,5,0"
                            Name="MachiningDataManageToggleButton"
                            Content="{DynamicResource btnMachiningDataManage}"
                        Command="{x:Static GD_Popup:DraggablePopup.PopupCommand}"  
                                           CommandTarget="{Binding ElementName=WorkControlDraggablePopup}"
                            >

                        </Button>

                        <GD_Popup:DraggablePopup  Name="WorkControlDraggablePopup"  AllowTopMost="False"  IsTopmost="False"
                                              AllowsTransparency="True" Margin="16"   
                                             PlacementRectangle="0,0,0,0"
                                             Placement="MousePoint"
                                             PlacementTarget="{Binding ElementName=MachiningDataManageToggleButton}"
                                             >
                            <Border MinWidth="1000"  MaxWidth="1200"
                                                CornerRadius="10"
                                               Background="{DynamicResource PrimaryHueDarkBrush}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
                                <GroupBox Header="{DynamicResource btnMachiningDataManage}" FontSize="30"  
                                          Style="{StaticResource MaterialDesignGroupBox}" Margin="5">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition Height="auto"/>
                                        </Grid.RowDefinitions>
                                        <StackPanel Orientation="Vertical" MaxWidth="{x:Static SystemParameters.PrimaryScreenWidth  }">
                                            <dxg:GridControl 
                                        Name="NotArrangeWorkGridControl"
                                        HorizontalAlignment="Stretch"     Grid.ColumnSpan="2"  
                                        Grid.Row="5" 
                                    Grid.Column="0" Margin="10,0"
                                    Background="Transparent"  Height="300"      
                                     CustomRowFilterCommand="{Binding NotArrangeWorkRowFilterCommand}"        
                                                Style="{StaticResource ProjectGridControl}"
                                    AutoGenerateColumns="None"  
                                 ItemsSource="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance}, IsAsync=True,
                                        Path=SelectedProjectDistributeVM.StampingBoxPartsVM.BoxPartsParameterVMObservableCollection}"
                               >
                                                <dxg:GridControl.View >
                                                    <dxg:TableView   
                                                        SearchString="{Binding ElementName=ParameterSearchTextBox, Path=Text, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                        IsColumnMenuEnabled ="False"
                                                        AllowSorting="false"
                                                        AllowColumnFiltering="False"
                                                        AllowColumnMoving="False"
                                                        AllowEditing="False"
                                                        ShowColumnHeaders="True"
                                                        HeaderHorizontalAlignment="Center"
                                                        VerticalScrollbarVisibility="Auto"
                                                        HorizontalScrollbarVisibility="Auto"
                                                        ShowGroupPanel="False"
                                                        ShowSearchPanelMode="Never"
                                                        SearchPanelNullText="Search Tasks(Ctrl + F)" 
                                                        SearchPanelFindMode="Always"
                                                        ShowIndicator="False"
                                                        SearchDelay="0"                            
                                                        AllowDragDrop="False"
                                                        AllowDrop="False"
                                                        ShowDragDropHint="False"
                                                    >


                                                    </dxg:TableView>
                                                </dxg:GridControl.View>

                                                <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_Box}"  FieldName="BoxIndex"  Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>



                                                <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_ProjectName}" FieldName="ProductProjectName" Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_Parameter_A}" FieldName="ParameterA" Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_Parameter_B}" FieldName="ParameterB" Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_Parameter_C_DataMartix}" FieldName="ParameterC" Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_MachiningForm}" FieldName="SettingBaseVM.NumberSettingMode"  Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>

                                                <dxg:GridColumn  
                            HorizontalHeaderContentAlignment="Left"  Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <Grid>
                                                                <ToggleButton Padding="0" Margin="1" x:Name="MachiningFormTButton">
                                                                    <ToggleButton.Style>
                                                                        <Style TargetType="ToggleButton" BasedOn="{StaticResource GD_MainToggleButton}">
                                                                            <Setter Property="Width" Value="auto"/>
                                                                            <Setter Property="Height" Value="auto"/>
                                                                            <Setter Property="BorderThickness" Value="0"/>
                                                                            <Setter Property="Template">
                                                                                <Setter.Value>
                                                                                    <ControlTemplate TargetType="ToggleButton">
                                                                                        <Grid>
                                                                                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                                                        </Grid>
                                                                                    </ControlTemplate>
                                                                                </Setter.Value>
                                                                            </Setter>
                                                                            <Style.Triggers>
                                                                                <Trigger Property="IsMouseOver" Value="True">
                                                                                    <Setter Property="IsChecked" Value="True"/>
                                                                                </Trigger>
                                                                                <Trigger Property="IsChecked" Value="True">
                                                                                    <Setter Property="Foreground" Value="Black"/>
                                                                                    <Setter Property="Background" Value="{StaticResource ToggleButtonBackgroundSkyBlueBrush}"/>
                                                                                </Trigger>
                                                                            </Style.Triggers>
                                                                        </Style>
                                                                    </ToggleButton.Style>
                                                                    <GD_SchematicDiagram:QRPlateRowDiagramControl     IsEnabled="False"
                                                                        DataContext="{Binding Row.SettingBaseVM}" RedMeasurementLineVisibility="Collapsed" >
                                                                        <GD_SchematicDiagram:QRPlateRowDiagramControl.LayoutTransform>
                                                                            <TransformGroup>
                                                                                <ScaleTransform ScaleX="1" ScaleY="1"/>
                                                                            </TransformGroup>
                                                                        </GD_SchematicDiagram:QRPlateRowDiagramControl.LayoutTransform>
                                                                    </GD_SchematicDiagram:QRPlateRowDiagramControl>
                                                                    <!--
                                                                    <Grid  RenderTransformOrigin="0.5,0.5">
                                                                        <Grid.LayoutTransform>
                                                                            <ScaleTransform ScaleX="0.12" ScaleY="0.12"/>
                                                                        </Grid.LayoutTransform>
                                                                        
                                                                        <GD_SchematicDiagram:PlateRowDiagramViewUserControl
                            DataContext="{Binding Row.SettingBaseVM}" 
                           Visibility="{Binding Converter={GD_Common:ObjectIsEqualToVisibilityConverter IsTypeJudge=True} , ConverterParameter={x:Type GD_ParameterSettingViewModels:NumberSettingViewModel}}"
                            RedMeasurementLineVisibility="Collapsed"/>
                                                                        <GD_SchematicDiagram:PlateRowDiagramWithQRViewUserControl
                            DataContext="{Binding Row.SettingBaseVM}" 
                                Visibility="{Binding Converter={GD_Common:ObjectIsEqualToVisibilityConverter IsTypeJudge=True} , ConverterParameter={x:Type GD_ParameterSettingViewModels:QRSettingViewModel}}"
                            RedMeasurementLineVisibility="Collapsed"/>
                                                                    </Grid>-->
                                                                </ToggleButton>

                                                            </Grid>

                                                        </DataTemplate>
                                                    </dxg:GridColumn.CellTemplate>
                                                </dxg:GridColumn>


                                                <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header=""  FieldName="MachiningStatus"  Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>





                                            </dxg:GridControl>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="10">
                                                <Button Command="{Binding ArrangeWorkCommand}" Margin="5,0"
                                            CommandParameter="{Binding ElementName=NotArrangeWorkGridControl, Path=SelectedItems}" Content="{materialDesign:PackIcon Kind=ArrowDownBold}"/>
                                                <Button Command="{Binding ArrangeWorkCommand}"  Margin="5,0"
                                            CommandParameter="{Binding ElementName=NotArrangeWorkGridControl, Path=ItemsSource}" Content="{materialDesign:PackIcon Kind=TransferDown}"/>

                                                <Button Command="{Binding CancelArrangeWorkCommand}"  Margin="5,0"
                                            CommandParameter="{Binding ElementName=ArrangeWorkGridControl, Path=SelectedItems}" Content="{materialDesign:PackIcon Kind=ArrowUpBold}"/>
                                                <Button Command="{Binding CancelArrangeWorkCommand}"  Margin="5,0"
                                            CommandParameter="{Binding ElementName=ArrangeWorkGridControl, Path=ItemsSource}" Content="{materialDesign:PackIcon Kind=TransferUp}"/>

                                            </StackPanel>
                                            <dxg:GridControl 
                                        Name="ArrangeWorkGridControl" HorizontalAlignment="Stretch"     Grid.ColumnSpan="2" 
                                        Grid.Row="5" 
                                    Grid.Column="0" Margin="10,0"
                                    Background="Transparent"  Height="300" 
                                     CustomRowFilterCommand="{Binding ArrangeWorkRowFilterCommand}"
                                    Style="{StaticResource ProjectGridControl}"
                                    AutoGenerateColumns="None" 
                                 ItemsSource="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance}, IsAsync=True,
                                                Path=SelectedProjectDistributeVM.StampingBoxPartsVM.BoxPartsParameterVMObservableCollection}"
                                     >
                                                <dxg:GridControl.View >
                                                    <dxg:TableView  
                                                        SearchString="{Binding ElementName=ParameterSearchTextBox, Path=Text, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                        IsColumnMenuEnabled ="False"
                                                        AllowSorting="False"
                                                        AllowColumnFiltering="False"
                                                        AllowColumnMoving="False"
                                                        AllowEditing="False"
                                                        ShowColumnHeaders="True"
                                                        HeaderHorizontalAlignment="Center"
                                                        VerticalScrollbarVisibility="Auto"
                                                        HorizontalScrollbarVisibility="Auto"
                                                        ShowGroupPanel="False"
                                                        ShowSearchPanelMode="Never"
                                                        SearchPanelNullText="Search Tasks(Ctrl + F)" 
                                                        SearchPanelFindMode="Always"
                                                        ShowIndicator="False"
                                                        SearchDelay="0"                            
                                                        AllowDragDrop="False"
