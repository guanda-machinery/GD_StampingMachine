<UserControl x:Class="GD_StampingMachine.UserControls.MachineMonitorUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GD_StampingMachine"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="800" 
             
             xmlns:CalcB="clr-namespace:CalcBinding;assembly=CalcBinding" 
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" 
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"                       
             xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling"          
             xmlns:GD_ViewModels="clr-namespace:GD_StampingMachine.ViewModels"
             xmlns:GD_ProductSettingViewModels="clr-namespace:GD_StampingMachine.ViewModels.ProductSetting"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:GD_StampingMachine_UserControls="clr-namespace:GD_StampingMachine.UserControls"
             
             xmlns:GD_Singletons="clr-namespace:GD_StampingMachine.Singletons" 
             
             xmlns:GD_SchematicDiagram="clr-namespace:GD_StampingMachine.UserControls.NumberSettingSchematicDiagram" 
             xmlns:GD_ParameterSettingViewModels="clr-namespace:GD_StampingMachine.ViewModels.ParameterSetting" 
             xmlns:GD_CommonControl="clr-namespace:GD_CommonControlLibrary;assembly=GD_CommonControlLibrary" xmlns:c="clr-namespace:CalcBinding;assembly=CalcBinding"
             d:DataContext="{d:DesignInstance Type=GD_ViewModels:MachineMonitorViewModel}"
             
              xmlns:sys="clr-namespace:System;assembly=mscorlib" 
             xmlns:GD_Popup="clr-namespace:GD_CommonControlLibrary.GD_Popup;assembly=GD_CommonControlLibrary" 
             xmlns:MachineMonitorViewModel="clr-namespace:GD_StampingMachine.ViewModels.MachineMonitor"
             Name="MachineMonitorUC"
             >


    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" MinHeight="50" MaxHeight="{Binding ElementName=PrimaryColorZone, Path=ActualHeight}"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <ScrollViewer VerticalScrollBarVisibility="Auto">

            <materialDesign:ColorZone  Name="PrimaryColorZone"
                Mode="PrimaryDark"  >
            <materialDesign:ColorZone.Effect>
                <DropShadowEffect  ShadowDepth="2"
      Direction="270"
      Color="Black"
      Opacity="0.5"
     BlurRadius="6"/>
            </materialDesign:ColorZone.Effect>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                </Grid.RowDefinitions>
                <!--TextBlock  Text="{Binding ProjectDistributeVMSelected.ProjectDistributeName}" Grid.ColumnSpan="2" FontSize="30"/-->
                <StackPanel Grid.Row="0"  Grid.Column="1"  Orientation="Horizontal" HorizontalAlignment="Center" >
                        <Border  
                            BorderBrush="#e82127" BorderThickness="0,0,0,5" CornerRadius="0" Width="200" Height="200"  >
                            <!--Image Source="pack://application:,,,/image/Machine/Machine_QR_Dark.svg"  Margin="5"/-->
                            <Image Margin="5">
                                <Image.Style>
                                    <Style TargetType="Image">
                                        <Setter Property="Source" Value="{StaticResource QR_Machine_DarkDrawingImage}"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                <Setter Property="Source" Value="{StaticResource  QR_Machine_LightDrawingImage}"/>
                                            </DataTrigger>

                                        </Style.Triggers>
                                    </Style>

                                </Image.Style>
                            </Image>
                        </Border>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <StackPanel.Resources>
                                <Style TargetType="materialDesign:PackIcon">
                                    <Setter Property="Width" Value="50"/>
                                    <Setter Property="Height" Value="50"/>
                                    <Setter Property="Foreground" Value="#e82127"/>
                                <Setter Property="Margin" Value="5"/>
                            </Style>
                        </StackPanel.Resources>
                        <materialDesign:PackIcon Kind="ChevronDoubleRight" />
                            <materialDesign:PackIcon Kind="ChevronDoubleRight" />
                        </StackPanel>
                    <Border  BorderBrush="#ff9900" BorderThickness="0,0,0,5" CornerRadius="0" Width="200" Height="200"  >
                            <Image Margin="5">
                                <Image.Style>
                                    <Style TargetType="Image">
                                        <Setter Property="Source" Value="{StaticResource Stamper_DarkDrawingImage}"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                <Setter Property="Source" Value="{StaticResource  Stamper_LightDrawingImage}"/>
                                            </DataTrigger>

                                        </Style.Triggers>
                                    </Style>

                                </Image.Style>
                            </Image>
                        </Border>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <StackPanel.Resources>
                                <Style TargetType="materialDesign:PackIcon">
                                    <Setter Property="Width" Value="50"/>
                                    <Setter Property="Height" Value="50"/>
                                    <Setter Property="Foreground" Value="#ff9900"/>
                                <Setter Property="Margin" Value="5"/>
                            </Style>
                        </StackPanel.Resources>
                        <materialDesign:PackIcon Kind="ChevronDoubleRight" />
                            <materialDesign:PackIcon Kind="ChevronDoubleRight" />
                        </StackPanel>
                        <Border  BorderBrush="#25cb55" BorderThickness="0,0,0,5" CornerRadius="0" Width="200" Height="200"  >
                            <Image Margin="5">
                                <Image.Style>
                                    <Style TargetType="Image">
                                        <Setter Property="Source" Value="{StaticResource Cutter_DarkDrawingImage}"/>

                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                <Setter Property="Source" Value="{StaticResource  Cutter_LightDrawingImage}"/>
                                            </DataTrigger>

                                        </Style.Triggers>
                                    </Style>

                                </Image.Style>
                            </Image>
                        </Border>
                </StackPanel>

                <Border Grid.Row="1" Margin="10" Background="{DynamicResource PrimaryHueLightForegroundBrush}"/>

                    <Border Grid.Row="2" Background="White" Margin="0,0,0,0" />
                    <Viewbox Grid.ColumnSpan="3"    Grid.Row="2" MaxHeight="120">
                    <Grid >
                            <Border Background="White" Margin="0,0,5,0"/>
                            <Grid HorizontalAlignment="Right" MinHeight="100">
                                <Grid.Resources>
                                    <Style TargetType="Border" x:Key="MachineSettingBorder">
                                        <Setter Property="Background" Value="LightGray"/>
                                                <Style.Triggers>
                                                    <MultiDataTrigger>
                                                        <MultiDataTrigger.Conditions>
                                                            <Condition  Binding="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance}, Path=IsConnected}" 
                      Value="true" />
                                                            <Condition  Binding="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance}, Path=Cylinder_HydraulicEngraving_IsStopDown}" 
                      Value="true" />
                                                        </MultiDataTrigger.Conditions>
                                                        <Setter Property="Background"  Value="Green"/>
                                                    </MultiDataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                </Grid.Resources>

                                <Border Style="{StaticResource MachineSettingBorder}"
                                    Visibility="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance},
                                    Path=PlateBaseObservableCollection[9].EngravingIsFinish,
                                    Converter={GD_CommonControl:BooleanToVisibilityConverter Invert=False} , ConverterParameter={x:Static Visibility.Hidden}}"
                                    Width="47.5" HorizontalAlignment="Right" Margin="0,0,475,0" />

                                <Border  Style="{StaticResource MachineSettingBorder}"
                                    Visibility="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance},
                                    Path=PlateBaseObservableCollection[9].EngravingIsFinish,
                                    Converter={GD_CommonControl:BooleanToVisibilityConverter Invert=True} , ConverterParameter={x:Static Visibility.Hidden}}"
                                    Width="47.5" HorizontalAlignment="Right" Margin="0,0,428,0" />


                                <Border  Margin="0,0,5,0" Width="48"  HorizontalAlignment="Right" >
                                    <Border.Style>
                                        <Style TargetType="Border" >
                                            <Setter Property="Background" Value="Gray"/>
                                            <Style.Triggers>
                                                <MultiDataTrigger>
                                                    <MultiDataTrigger.Conditions>
                                                        <Condition  Binding="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance}, Path=IsConnected}" 
                                                     Value="true" />
                                                        <Condition  Binding="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance}, Path=Cylinder_HydraulicCutting_IsCutPoint}" 
                                                     Value="true" />
                                                    </MultiDataTrigger.Conditions>
                                                    <Setter Property="Background"  Value="Green"/>
                                                </MultiDataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>

                                </Border>
                            </Grid>
                        
                        
                        
                        
                        
                            <ItemsControl  
                                HorizontalAlignment="Right" Margin="5,0"   FlowDirection="RightToLeft"
                                ItemsSource="{Binding Source={x:Static GD_Singletons:StampMachineDataSingleton.Instance},
                                Path=PlateBaseObservableCollection, IsAsync=True}"
                                                             ScrollViewer.VerticalScrollBarVisibility="Disabled"     
                                 ScrollViewer.HorizontalScrollBarVisibility="Disabled"  >
                                <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <Grid  Margin="5,5" >
                                            <Grid.Resources>
                                                <Style TargetType= "{x:Type GD_SchematicDiagram:QRPlateRowDiagramControl}" >
                                                    <Setter Property="PlateRowBackground" Value="{DynamicResource NoneIsFinishSolidColorBrush}"/>
                                                    <Setter Property="IsEnabled" Value="False"/>
                                                    <Setter Property="Visibility" Value="Visible"/>
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding DataContext.DataMatrixIsFinish, RelativeSource={RelativeSource Mode=TemplatedParent}}" Value="true">
                                                            <Setter Property="PlateRowBackground" Value="{DynamicResource DataMatrixIsFinishSolidColorBrush}"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding DataContext.EngravingIsFinish, RelativeSource={RelativeSource Mode=TemplatedParent}}" Value="true">
                                                            <Setter Property="PlateRowBackground" Value="{DynamicResource ShearingIsFinishSolidColorBrush}"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding DataContext.ShearingIsFinish, RelativeSource={RelativeSource Mode=TemplatedParent}}" Value="true">
                                                            <Setter Property="PlateRowBackground" Value="{DynamicResource EngravingIsFinishSolidColorBrush}"/>
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Grid.Resources>                                      
                                        <Border VerticalAlignment="Center" FlowDirection="LeftToRight"
                                            BorderThickness="1" BorderBrush="Black" CornerRadius="2" Padding="0" 
                                            HorizontalAlignment="Center">
                                            <ToggleButton Padding="0" Margin="0" x:Name="MachiningFormTButton" Opacity="0.95" >
                                                <ToggleButton.Style>
                                                    <Style TargetType="ToggleButton" BasedOn="{StaticResource GD_MainToggleButton}">
                                                        <Setter Property="BorderThickness" Value="0"/>
                                                        <Setter Property="Template">
                                                            <Setter.Value>
                                                                <ControlTemplate TargetType="ToggleButton">
                                                                    <Grid>
                                                                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                                    </Grid>
                                                                </ControlTemplate>
                                                            </Setter.Value>
                                                        </Setter>

                                                    </Style>
                                                </ToggleButton.Style>
                                                <GD_SchematicDiagram:QRPlateRowDiagramControl   
                                                        IsEnabled="False"    
                                                                        DataContext="{Binding SettingBaseVM}" RedMeasurementLineVisibility="Collapsed" >
                                                    <GD_SchematicDiagram:QRPlateRowDiagramControl.LayoutTransform>
                                                        <TransformGroup>
                                                            <RotateTransform Angle="90"/>
                                                        </TransformGroup>
                                                    </GD_SchematicDiagram:QRPlateRowDiagramControl.LayoutTransform>

                                                </GD_SchematicDiagram:QRPlateRowDiagramControl>
                                            </ToggleButton>
                                        </Border>
                                        <GD_Popup:DraggablePopup StaysOpen="False"
                                                                        PopupAnimation="Fade"   FlowDirection="LeftToRight"  
                                                                        Placement="Mouse"    PlacementRectangle="-20,40,0,0"
                                                                        PlacementTarget="{Binding ElementName=MachiningFormTButton}"  
                                                                        HorizontalAlignment="Center" 
                                                                        IsOpen="{Binding ElementName=MachiningFormTButton, Path=IsChecked, Mode=TwoWay}" >

                                                    <Grid Width="400">
                                                            <Viewbox>
                                                                <GD_SchematicDiagram:QRPlateRowDiagramControl    
                                                                    DataContext="{Binding SettingBaseVM , IsAsync=True}" RedMeasurementLineVisibility="Collapsed" >
                                                                </GD_SchematicDiagram:QRPlateRowDiagramControl>
                                                            </Viewbox>
                                                        </Grid>
                                                </GD_Popup:DraggablePopup>

                                    </Grid>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Horizontal" />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                        </ItemsControl>
                    </Grid>

                    </Viewbox>
                    <StackPanel Orientation="Horizontal" Grid.Row="3" Grid.ColumnSpan="3">

                        <Button  Margin="5,0" Padding="5,2,5,0"
                            Name="MachiningDataManageToggleButton"
                            Content="{DynamicResource btnMachiningDataManage}"                 
                        Command="{x:Static GD_Popup:DraggablePopup.PopupCommand}"  
                                 CommandParameter="{StaticResource TrueValue}"
                                           CommandTarget="{Binding ElementName=WorkControlDraggablePopup}"
                            >

                        </Button>



                        <GD_Popup:DraggablePopup  
                            MinHeight="100" MinWidth="100"
                            Name="WorkControlDraggablePopup" 
                                                 StaysOpen="True" 
                                                  AllowTopMost="False"
                                              AllowsTransparency="True" Margin="16"   
                                             PlacementRectangle="0,0,0,0"
                                             Placement="MousePoint"
                                             PlacementTarget="{Binding ElementName=MachiningDataManageToggleButton}"
                                             >
                            <GD_Popup:DraggablePopup.Resources>
                                <Style TargetType="Button" BasedOn="{StaticResource SiegCustomButton_Primary}">
                                    <Setter Property="MinWidth" Value="80"/>

                                </Style>

                            </GD_Popup:DraggablePopup.Resources>
                            <Border Width="1000"  
                                                CornerRadius="10"
                                               Background="{DynamicResource PrimaryHueDarkBrush}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
                                <GroupBox Header="{DynamicResource btnMachiningDataManage}" FontSize="30"  
                                          Style="{StaticResource MaterialDesignGroupBox}" Margin="5">

                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                        </Grid.RowDefinitions>

                                        <dxg:GridControl Grid.Row="0"  
                                        Name="NotArrangeWorkGridControl"
                                        HorizontalAlignment="Stretch"     
                                    Grid.Column="0" Margin="10,0"
                                    Background="Transparent"  Height="300"      
                                   
                                                Style="{StaticResource ProjectGridControl}"
                                    AutoGenerateColumns="None"  
                                 ItemsSource="{Binding SelectedProjectDistributeVM.StampingBoxPartsVM.SeparateBoxVMObservableCollection.UnscheduledPartsParameterCollection ,IsAsync=True}"
                               >
                                                <dxg:GridControl.View >
                                                    <dxg:TableView   
                                                        IsColumnMenuEnabled ="False"
                                                        AllowSorting="false"
                                                        AllowColumnFiltering="False"
                                                        AllowColumnMoving="False"
                                                        AllowEditing="False"
                                                        ShowColumnHeaders="True"
                                                        HeaderHorizontalAlignment="Center"
                                                        VerticalScrollbarVisibility="Auto"
                                                        HorizontalScrollbarVisibility="Auto"
                                                        ShowGroupPanel="False"
                                                        ShowSearchPanelMode="Never"
                                                        SearchPanelNullText="Search Tasks(Ctrl + F)" 
                                                        SearchPanelFindMode="Always"
                                                        ShowIndicator="False"
                                                        SearchDelay="0"                            
                                                        AllowDragDrop="False"
                                                        AllowDrop="False"
                                                        ShowDragDropHint="False"
                                                    >


                                                    </dxg:TableView>
                                                </dxg:GridControl.View>

                                                <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_Box}"  FieldName="BoxIndex"  Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                            <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_ProjectName}" FieldName="ProductProjectName" Width="auto" AllowMoving="false"  IsSmart="True">
                                                <dxg:GridColumn.EditSettings>
                                                    <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                </dxg:GridColumn.EditSettings>
                                            </dxg:GridColumn>
                                            <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_Parameter_A}" FieldName="ParameterA" Width="auto" AllowMoving="false"  IsSmart="True">
                                                <dxg:GridColumn.EditSettings>
                                                    <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                </dxg:GridColumn.EditSettings>
                                            </dxg:GridColumn>
                                            <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_Parameter_B}" FieldName="ParameterB" Width="auto" AllowMoving="false"  IsSmart="True">
                                                <dxg:GridColumn.EditSettings>
                                                    <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                </dxg:GridColumn.EditSettings>
                                            </dxg:GridColumn>
                                            <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_Parameter_C_DataMartix}" FieldName="ParameterC" Width="auto" AllowMoving="false"  IsSmart="True">
                                                <dxg:GridColumn.EditSettings>
                                                    <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                </dxg:GridColumn.EditSettings>
                                            </dxg:GridColumn>
                                            <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header="{DynamicResource Text_MachiningForm}" FieldName="SettingBaseVM.NumberSettingMode"  Width="auto" AllowMoving="false"  IsSmart="True">
                                                <dxg:GridColumn.EditSettings>
                                                    <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                </dxg:GridColumn.EditSettings>
                                            </dxg:GridColumn>

                                            <dxg:GridColumn  
                            HorizontalHeaderContentAlignment="Left"  Width="auto" AllowMoving="false"  IsSmart="True">
                                                <dxg:GridColumn.EditSettings>
                                                    <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                </dxg:GridColumn.EditSettings>
                                                <dxg:GridColumn.CellTemplate>
                                                    <DataTemplate>

                                                        <Grid DataContext="{Binding Row}">
                                                            <GD_SchematicDiagram:QRPlateRowDiagramControl   DataContext="{Binding SettingBaseVM , IsAsync=True}"   IsEnabled="False"
                                                                      RedMeasurementLineVisibility="Collapsed" >
                                                            </GD_SchematicDiagram:QRPlateRowDiagramControl>
                                                        </Grid>

                                                    </DataTemplate>
                                                </dxg:GridColumn.CellTemplate>
                                            </dxg:GridColumn>



                                            <dxg:GridColumn  HorizontalHeaderContentAlignment="Left"  Header=""  FieldName="MachiningStatus"  Width="auto" AllowMoving="false"  IsSmart="True">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings HorizontalContentAlignment="Left"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>





                                            </dxg:GridControl>
                                        <StackPanel Grid.Row="1"  
                                                Orientation="Horizontal" HorizontalAlignment="Center" Margin="10">
                                                <Button Command="{Binding ArrangeWorkCommand}" Margin="5,0"  
                                            CommandParameter="{Binding ElementName=NotArrangeWorkGridControl, Path=SelectedItems}" >
                                                    <Image VerticalAlignment="Center"  Height="15">
                                                        <Image.Style>
                                                            <Style TargetType="Image">
                                                                <Setter Property="Source" Value="{StaticResource Arrow_Down_WhiteDrawingImage}"/>
                                                                <Style.Triggers>
                                                                    <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                                        <Setter Property="Source" Value="{StaticResource Arrow_Down_BlackDrawingImage}"/>
                                                                    </DataTrigger>
                                                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button},Path=IsMouseOver}" Value="true">
                                                                        <Setter Property="Source" Value="{StaticResource Arrow_Down_BlackDrawingImage}"/>
                                                                    </DataTrigger>
                                                                </Style.Triggers>
                                                            </Style>
                                                        </Image.Style>
                                                    </Image>
                                                </Button>
                                                <Button Command="{Binding ArrangeWorkCommand}"  Margin="5,0"
                                            CommandParameter="{Binding ElementName=NotArrangeWorkGridControl, Path=ItemsSource}" >
                                                    <StackPanel Orientation="Horizontal">
                                                        <Image VerticalAlignment="Center"  Height="15">
                                                            <Image.Style>
                                                                <Style TargetType="Image">
                                                                    <Setter Property="Source" Value="{StaticResource Arrow_Down_WhiteDrawingImage}"/>
                                                                    <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                                            <Setter Property="Source" Value="{StaticResource Arrow_Down_BlackDrawingImage}"/>
                                                                        </DataTrigger>
                                                                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button},Path=IsMouseOver}" Value="true">
                                                                            <Setter Property="Source" Value="{StaticResource Arrow_Down_BlackDrawingImage}"/>
                                                                        </DataTrigger>
                                                                    </Style.Triggers>
                                                                </Style>
                                                            </Image.Style>
                                                        </Image>
                                                        <TextBlock Text="ALL" Padding="5,2,0,0" />
                                                    </StackPanel>


                                                </Button>
                                                <Button Command="{Binding CancelArrangeWorkCommand}"  Margin="5,0"
                                            CommandParameter="{Binding ElementName=ArrangeWorkGridControl, Path=SelectedItems}">
                                                    <Image VerticalAlignment="Center"  Height="15">
                                                        <Image.Style>
                                                            <Style TargetType="Image">
                                                                <Setter Property="Source" Value="{StaticResource Arrow_Up_WhiteDrawingImage}"/>
                                                                <Style.Triggers>
                                                                    <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                                        <Setter Property="Source" Value="{StaticResource Arrow_Up_BlackDrawingImage}"/>
                                                                    </DataTrigger>
                                                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button},Path=IsMouseOver}" Value="true">
                                                                        <Setter Property="Source" Value="{StaticResource Arrow_Up_BlackDrawingImage}"/>
                                                                    </DataTrigger>
                                                                </Style.Triggers>
                                                            </Style>
                                                        </Image.Style>
                                                    </Image>
                                                </Button>
                                                <Button Command="{Binding CancelArrangeWorkCommand}"  Margin="5,0"
                                            CommandParameter="{Binding ElementName=ArrangeWorkGridControl, Path=ItemsSource}" >
                                                    <StackPanel Orientation="Horizontal">
                                                        <Image VerticalAlignment="Center"  Height="15">
                                                            <Image.Style>
                                                                <Style TargetType="Image">
                                                                    <Setter Property="Source" Value="{StaticResource Arrow_Up_WhiteDrawingImage}"/>
                                                                    <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding Source={x:Static GD_Singletons:StampingMachineSingleton.Instance},Path=IsBrightMode}" Value="true">
                                                                            <Setter Property="Source" Value="{StaticResource Arrow_Up_BlackDrawingImage}"/>
                                                                        </DataTrigger>
                                                                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button},Path=IsMouseOver}" Value="true">
                                                                            <Setter Property="Source" Value="{StaticResource Arrow_Up_BlackDrawingImage}"/>
                                                                        </DataTrigger>
                                                                    </Style.Triggers>
                                                                </Style>
                                                            </Image.Style>
                                                        </Image>
                                                        <TextBlock Text="ALL" Padding="5,2,0,0" />
                                                    </StackPanel>
                                                </Button>

                                            <ToggleButton Name="TestToggle"  Style="{StaticResource GD_ToggleButton_Gray}" IsChecked="{Binding TestedIsChecked}"
                                                Content="Test" Visibility="{Binding IsDebuggerAttached , Converter={dx:BooleanToVisibilityConverter}}">
                                                
                                            </ToggleButton>

                                            <ToggleButton Style="{StaticResource GD_ToggleButton_Gray}"
                        IsChecked="{Binding ShowIsTransported}"
                                     Content="{DynamicResource btnShowFinishItem}"
                                  Padding="5,3,5,0" Margin="5,5,5,5"/>


                                        </StackPanel>
                                        <dxg:GridControl Grid.Row="2"  
                                        Name="ArrangeWorkGridControl" HorizontalAlignment="Stretch" 
                                    Grid.Column="0" Margin="10,0"
                                    Background="Transparent"  Height="300" 
                                    Style="{StaticResource ProjectGridControl}"
                                                         CustomRowFilterCommand="{Binding BoxPartsParameterVMRowFilterCommand}"
                                    AutoGenerateColumns="None" 
                                 ItemsSource="{Binding SelectedProjectDistributeVM.StampingBoxPartsVM.SeparateBoxVMObservableCollection.ScheduledPartsParameterCollection, IsAsync=True}"
                                     >
                                                <dxg:GridControl.View >
                                                    <dxg:TableView  
                                                            IsColumnMenuEnabled ="False"
                                                        AllowSorting="False"
                                                        AllowColumnFiltering="False"
                                                        AllowColumnMoving="False"
                                                        AllowEditing="False"
                                                        ShowColumnHeaders="True"
                                                        HeaderHorizontalAlignment="Center"
                                                        VerticalScrollbarVisibility="Auto"
                                                        HorizontalScrollbarVisibility="Auto"
                                                        ShowGroupPanel="False"
                                                        ShowSearchPanelMode="Never"
                                                        SearchPanelNullText="Search Tasks(Ctrl + F)" 
                                                        SearchPanelFindMode="Always"
                                                        ShowIndicator="False"
                                                        SearchDelay="0"                            
                                                        AllowDragDrop="False"
                                                        AllowDrop="False"
                                                        ShowDragDropHint="False"
